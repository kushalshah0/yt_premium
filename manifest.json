{
    "manifest_version": 3,
    "name": "YouTube Premium",
    "description": "Changes the YouTube to premium version.",
    "version": "1.0",
    "icons": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    },
    "content_scripts": [
      {
        "matches": ["https://www.youtube.com/*"],
        "js": ["content.js"],
        "run_at": "document_end"
      }
    ],
    "permissions": [
      "activeTab"
    ]
  }